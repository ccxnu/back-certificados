#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/non/Dev/Web/Tikke/certificados/node_modules/.pnpm/handlebars@4.7.8/node_modules/handlebars/bin/node_modules:/home/non/Dev/Web/Tikke/certificados/node_modules/.pnpm/handlebars@4.7.8/node_modules/handlebars/node_modules:/home/non/Dev/Web/Tikke/certificados/node_modules/.pnpm/handlebars@4.7.8/node_modules:/home/non/Dev/Web/Tikke/certificados/node_modules/.pnpm/node_modules:/home/non/Dev/Web/Tikke/certificados/node_modules:/home/non/Dev/Web/Tikke/node_modules:/home/non/Dev/Web/node_modules:/home/non/Dev/node_modules:/home/non/node_modules:/home/node_modules:/node_modules:/home/non/Dev/Web/Tikke/certificados/backend/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/home/non/Dev/Web/Tikke/certificados/node_modules/.pnpm/handlebars@4.7.8/node_modules/handlebars/bin/node_modules:/home/non/Dev/Web/Tikke/certificados/node_modules/.pnpm/handlebars@4.7.8/node_modules/handlebars/node_modules:/home/non/Dev/Web/Tikke/certificados/node_modules/.pnpm/handlebars@4.7.8/node_modules:/home/non/Dev/Web/Tikke/certificados/node_modules/.pnpm/node_modules:/home/non/Dev/Web/Tikke/certificados/node_modules:/home/non/Dev/Web/Tikke/node_modules:/home/non/Dev/Web/node_modules:/home/non/Dev/node_modules:/home/non/node_modules:/home/node_modules:/node_modules:/home/non/Dev/Web/Tikke/certificados/backend/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../handlebars/bin/handlebars" "$@"
else
  exec node  "$basedir/../handlebars/bin/handlebars" "$@"
fi
